

<div id="page-wrapper">
  <div class="container-fluid">
    <div class="row">
      <div class="box">
        <div class="col-lg-12 text-center">
          <!-- Controls -->
          <h2 class="brand-before">
            <small>
              Here is your Search/Check Reports
            </small>
          </h2>
          <hr class="tagline-divider">
        </div>
      </div>
    </div>
    <div class="row">
      <div class="form-box col-md-4  col-md-offset-3 ">
        <% @files.each do |file| %>
            <%= link_to 'Download IMG', load_docx_path(file[:id]), data: {"no-turbolinks" => true } %>
        <% end %>
      </div>
    </div>
  </div>
</div>
