<div class="row">
  <div class = "container">
    <div class="col-md-4 col-md-offset-4">
      <h1> ERROR 404 | PAGE NOT FOUND </h1>
      <% if current_user.nil? %>
          <%= link_to "Back to home", root_path, type: "button", class: "btn btn-default btn-lg btn-block"%>
      <% else %>
          <%= link_to "Back to dashboard", user_path(current_user), type: "button", class: "btn btn-default btn-lg btn-block"%>
      <% end %>
    </div>
  </div>
</div>